# Flask secret key for sessions
FLASK_SECRET_KEY=your_secret_key_here

# OpenVPN Easy-RSA password
EASYRSA_PASSWORD=your_easyrsa_password_here

# Admin credentials (SHA256 hash of password)
ADMIN_USERNAME=admin
ADMIN_PASSWORD_HASH=your_password_hash_here

# OpenVPN data directories
VPN_DATA=/opt/vpn-data
PKI_ISSUED=/opt/vpn-data/pki/issued
